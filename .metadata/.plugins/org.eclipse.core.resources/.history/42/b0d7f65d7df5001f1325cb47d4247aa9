
public class Ex03연산자 {

	public static void main(String[] args) {
		
		//산술연산자 : + - * / %
		int num1 = 10;
		int num2 = 3;
		
		System.out.println(num1 + num2);
		System.out.println(num1 - num2);
		System.out.println(num1 * num2);
		System.out.println(num1 / num2);
		System.out.println(num1 % num2);
		
		float num3 = 3.45f;
		System.out.println(num1+num3);
		
		

	}

}
